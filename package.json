{
  "name": "@devlander/hooks",
  "version": "0.0.124",
  "description": "Hooks used in projects",
  "main": "dist/umd/index.js",
  "scripts": {
    "refresh-modules": "node ./external-modules/refresh-modules.js",
    "build:index": "node ./external-modules/auto-export.js",
    "increment-version": "yarn version --patch",
    "lint": "eslint .",
    "build:esm": "tsc",
    "build": "yarn run build:index && yarn build:esm && yarn build:cjs",
    "build:cjs": "yarn run increment-version && rollup -c ",
    "timeout": "delay 5",
    "push-main": "git add -A && yarn run timeout && git commit -m 'updated build' && yarn run timeout  && git push origin main",
    "rollup": "npx rollup -c rollup.config.mjs",
    "test": "jest",
    "prepare": "yarn run build",
    "patch": "yarn version --new-version patch",
    "analyze": "size-limit --why",
    "start-publish": " yarn run build  && yarn version --patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devlander-Software/hooks.git"
  },
  "keywords": [
    "react",
    "react-native",
    "typescript",
    "hooks",
    "devlander"
  ],
  "author": "Landon Johnson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Devlander-Software/hooks/issues"
  },
  "homepage": "https://github.com/Devlander-Software/hooks#readme",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "browser": "dist/umd/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "packageManager": "yarn@3.6.3",
  "dependencies": {
    "@babel/core": "^7.22.17",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-object-rest-spread": "^7.22.15",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@babel/runtime": "^7.22.15",
    "@devlander/collect-exports-for-bundle": "^1.1.68",
    "@devlander/hooks": "^0.0.26",
    "@devlander/shared-react-native-types": "^1.0.163",
    "@devlander/ts-config-base": "^1.0.1",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-auto-install": "^3.0.4",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-web": "0.19.9",
    "rollup": "^3.29.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-generate-git-version": "^1.0.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-swc-preserve-directives": "^0.5.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.28",
    "@types/react-native": "^0.72.3",
    "cli-progress": "^3.12.0"
  }
